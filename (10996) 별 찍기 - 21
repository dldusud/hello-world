#include <stdio.h>

int main()
{
	int N;
	scanf("%d", &N);

	for (int i = 0; i < N * 2; i++)
	{
		for (int j = 0; j < N; j++)
		{
			if (j % 2 == 0)
			{
				if (i % 2 == 0)
					printf("*");
				else
					printf(" ");
			}
			
			if (j % 2 == 1)
			{
				if (i % 2 == 1)
					printf("*");
				else
					printf(" ");
			}

		}
		printf("\n");
	}

	return 0;
}
