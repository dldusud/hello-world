#include <stdio.h>

int main()
{
	int N;
	scanf("%d", &N);

	for (int i = 0; i < N; i++)
	{
		for (int j = 0; j < N; j++)
		{
			if (i >= j)
				printf("*");
		}
		printf("\n");
	}

	for (int i = 0; i < N - 1; i++)
	{
		for (int j = 0; j < N - 1; j++)
		{
			if (j >= i)
				printf("*");
		}
		printf("\n");
	}

	return 0;
}
